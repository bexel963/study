package com.my.cppCopy;

public class Program {

	public static void main(String[] args) {

		final Point p1 = new Point(1, 1);
		final Point p2 = new Point(p1);
		
		p1.setX(-4);
		p1.setY(-8);
		
		System.out.printf("p1.x: %d, p1.y: %d%s",p1.getX(), p1.getY(), System.lineSeparator());
		System.out.printf("p2.x: %d, p2.y: %d%s",p2.getX(), p2.getY(), System.lineSeparator());
		
	}

}
